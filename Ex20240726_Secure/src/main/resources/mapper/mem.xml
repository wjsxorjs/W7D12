<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sist.db.mapper.MemMapper">

    <select id="login" parameterType="String" resultType="com.sist.db.vo.MemVO">
        SELECT *
        FROM `member_t`
        WHERE m_id = #{m_id}
    </select>

    <insert id="reg" parameterType="com.sist.secure.vo.MemVO">
        INSERT
        INTO `member_t`(m_id,m_pw,m_name)
        VALUES(#{m_id},#{m_pw},#{m_name})
    </insert>
</mapper>